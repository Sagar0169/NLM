<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".ui.fragment.national_livestock_mission_fragments.NLSIAFeedFodderFragment">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:scrollbars="none">

    <LinearLayout
        android:id="@+id/llParent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingHorizontal="@dimen/dp_10">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_7"
        android:background="@drawable/curve_all_corner_grey"
        android:orientation="vertical"
        android:paddingHorizontal="@dimen/dp_10">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:fontFamily="@font/lexend_bold"
            android:text="Manpower and Capacity Building:"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_16" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginVertical="@dimen/dp_10"
            android:background="@drawable/curve_back_grey"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/dp_10">

            <TextView
                android:id="@+id/tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:fontFamily="@font/opensans_semibold"
                android:text="No of AI technicians involved"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_16" />

            <EditText

                android:id="@+id/et1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:background="@drawable/curve_all_corner_grey"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:maxLength="500"
                android:overScrollMode="always"
                android:paddingHorizontal="@dimen/dp_10"
                android:paddingVertical="@dimen/dp_5"
                android:scrollbars="vertical"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_16" />

            <TextView
                android:id="@+id/tv2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:fontFamily="@font/opensans_semibold"
                android:text="Number of AI Technician Trained"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_16" />

            <EditText

                android:id="@+id/et2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:background="@drawable/curve_all_corner_grey"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:maxLength="500"
                android:overScrollMode="always"
                android:paddingHorizontal="@dimen/dp_10"
                android:paddingVertical="@dimen/dp_5"
                android:scrollbars="vertical"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_16" />

            <TextView
                android:id="@+id/tv3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:fontFamily="@font/opensans_semibold"
                android:text="Total No. of Paravet Trained"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_16" />

            <EditText

                android:id="@+id/et3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:background="@drawable/curve_all_corner_grey"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:maxLength="500"
                android:overScrollMode="always"
                android:paddingHorizontal="@dimen/dp_10"
                android:paddingVertical="@dimen/dp_5"
                android:scrollbars="vertical"
                android:textColor="@color/black"
                android:layout_marginBottom="@dimen/dp_10"
                android:textSize="@dimen/sp_16" />




        </LinearLayout>
    </LinearLayout> <TextView
        android:id="@+id/tvSendOtp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginHorizontal="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginBottom="@dimen/dp_10"
        android:background="@drawable/curve_fill_blue"
        android:clickable="true"
        android:fontFamily="@font/poppins_medium"
        android:paddingHorizontal="@dimen/dp_20"
        android:paddingVertical="@dimen/dp_10"
        android:text="Save and Next"
        android:textSize="20sp"
        android:textAlignment="center"
        android:textColor="@color/white" /></LinearLayout>
    </ScrollView></RelativeLayout>
</layout>